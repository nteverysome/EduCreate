{
  "name": "phaser3-plane-selector",
  "version": "1.0.0",
  "description": "Modern Phaser 3 plane selector game with Vite and TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build.js",
    "build:only": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json,css,scss}\"",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "ci": "npm run type-check && npm run lint && npm run test && npm run build:only",
    "start": "npm run dev"
  },
  "dependencies": {
    "phaser": "^3.90.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-legacy": "^4.1.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.0",
    "terser": "^5.20.0",
    "typescript": "^5.8.3",
    "vite": "^4.5.14",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "phaser3",
    "game",
    "typescript",
    "vite",
    "plane",
    "shooter",
    "html5"
  ],
  "author": "EduCreate Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nteverysome/EduCreate.git"
  },
  "bugs": {
    "url": "https://github.com/nteverysome/EduCreate/issues"
  },
  "homepage": "https://github.com/nteverysome/EduCreate#readme",
  "main": "index.js"
}
